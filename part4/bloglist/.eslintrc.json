{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "semi": "off",
      "linebreak-style": ["error", "windows"],
      "no-param-reassign": [2, { "props": false }]
    }
}
